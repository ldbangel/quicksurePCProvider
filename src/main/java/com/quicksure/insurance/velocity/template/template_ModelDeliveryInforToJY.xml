		<SHIPFEE></SHIPFEE>
		<SHIPMETHOD>01</SHIPMETHOD>
		<JQ_SEQUENCE_NO>$!{dataObj.getBaseinfor().getJqquerySequenceNo()}
		</JQ_SEQUENCE_NO>
		<SY_SEQUENCE_NO>$!{dataObj.getBaseinfor().getSyquerySequenceNo()}
		</SY_SEQUENCE_NO>
		<OBJECT_LIST>
			#if($!{dataObj.getBaseinfor().getJqapplicationno()}&&""!=$!{dataObj.getBaseinfor().getJqapplicationno()})
			<PARAMS>
				<EXPRESS_ITEM_TYPE>0</EXPRESS_ITEM_TYPE>
				<EXPRESS_ITEM>$!{dataObj.getBaseinfor().getJqapplicationno()}</EXPRESS_ITEM>
				<QUANTITY>1</QUANTITY>
				<NAME>投保单</NAME>
			</PARAMS>
			#end
			#if($!{dataObj.getBaseinfor().getJqpolicyno()}&&""!=$!{dataObj.getBaseinfor().getJqpolicyno()})
			<PARAMS>
				<EXPRESS_ITEM_TYPE>1</EXPRESS_ITEM_TYPE>
				<EXPRESS_ITEM>$!{dataObj.getBaseinfor().getJqpolicyno()}</EXPRESS_ITEM>
				<QUANTITY>1</QUANTITY>
				<NAME>保单</NAME>
			</PARAMS>
			#end
			#if($!{dataObj.getBaseinfor().getSyapplicationno()}&&""!=$!{dataObj.getBaseinfor().getSyapplicationno()})
			<PARAMS>
				<EXPRESS_ITEM_TYPE>2</EXPRESS_ITEM_TYPE>
				<EXPRESS_ITEM>$!{dataObj.getBaseinfor().getSyapplicationno()}</EXPRESS_ITEM>
				<QUANTITY>1</QUANTITY>
				<NAME>投保单</NAME>
			</PARAMS>
			#end
			#if($!{dataObj.getBaseinfor().getSypolicyno()}&&""!=$!{dataObj.getBaseinfor().getSypolicyno()})
