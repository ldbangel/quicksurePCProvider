		<TRANSTIME>$!{times}</TRANSTIME>
	</THIRD>
	<BODY>
		<DEPT_NO>$!{dataObj.getBaseinfor().getDeptno()}</DEPT_NO>
		<ORDER_NO>$!{dataObj.getBaseinfor().getOrderno()}</ORDER_NO>
		<INSRNT_CTCT_CNM>$!{dataObj.getBaseinfor().getInsrntname()}</INSRNT_CTCT_CNM>
